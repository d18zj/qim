@model QimErp.ServiceContracts.Dto.RegisterUserInput
@{

}
<form asp-controller="Account" asp-action="Register" asp-route-returnurl="@Url.Action("Register")" method="post" class="form-horizontal">
    <h4>Welcome to register.</h4>
    <hr />
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <label class="col-md-2 control-label" asp-for="TenantName" ></label>
        <div class="col-md-10">
            <input asp-for="TenantName" class="form-control" />
            <span asp-validation-for="TenantName" class="text-danger"></span>
        </div>
    </div>

    <div class="form-group">
        <label class="col-md-2 control-label" asp-for="CellPhone"></label>
        <div class="col-md-10">
            <input asp-for="CellPhone" class="form-control" />
            <span asp-validation-for="CellPhone" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <label class="col-md-2 control-label" asp-for="UserName"></label>
        <div class="col-md-10">
            <input asp-for="UserName" class="form-control" />
            <span asp-validation-for="UserName" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <button type="submit" class="btn btn-default">提交</button>
        </div>
    </div>
</form>

@section Scripts {
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}